[rrdb]
default_policy=1 sec FOR 1 min, 10 sec FOR 10 min, 1 min FOR 1 week, 10 min FOR 1 year, 1 hour FOR 10 years
path=@localstatedir@/@PACKAGE_NAME@
flush_interval=1 min

[server_tcp]
address=127.0.0.1
port=9876
max_message_size=4096
thread_pool_size=10

[server_udp]
address=127.0.0.1
port=9876
max_message_size=4096
thread_pool_size=5
send_response=true

[log]
level=debug
destination=@localstatedir@/run/@PACKAGE_NAME@/@PACKAGE_NAME@.log
time_format=%m/%d/%Y %H:%M:%S
